    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "http-shutdown": "^1.2.1",
    "mocha": "^10.2.0",
    "monet": "^0.9.0",
    "nodemon": "^1.19.1",
    "ramda": "^0.28.0",
    "rebote": "^1.0.2"
  },
  "devDependencies": {
    "chai": "^4.2.0",
